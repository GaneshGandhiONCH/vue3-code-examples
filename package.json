    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.1.0-alpha.13",
    "@storybook/addon-interactions": "^7.1.0-alpha.13",
    "@storybook/addon-links": "^7.1.0-alpha.13",
    "@storybook/blocks": "^7.1.0-alpha.13",
